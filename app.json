{
  "expo": {
    "name": "PicPay",
    "slug": "picpay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#21C25E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.patrckgratao.picpay"
    },
    "android": {
      "package": "com.patryckgratao.picpay"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
